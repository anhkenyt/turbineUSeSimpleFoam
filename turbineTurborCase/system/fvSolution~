FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "pcorr.*"
    {
        solver          GAMG;
        smoother        DICGaussSeidel;
        tolerance   1e-5;//daBefore   1e-5;
        relTol    0.01  ;   //daBefore0.01;
    }

    p
    {
        $pcorr;
        tolerance     1e-5 // 1e-05;
        relTol          0.01;
    }

    pFinal
    {
        $p;
        toletance 1e-5;
        relTol 0;
    }

    "(U|k|epsilon)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance     1e-8; // 1e-08;
        relTol         0;// 0.0;
    }

    "(U|k|epsilon)Final"
    {
        $U;
        //tolerance		1e-0
        relTol          0;
    }
}

PIMPLE
{
    correctPhi          yes;
    nOuterCorrectors    2;
    nCorrectors         1;
    nNonOrthogonalCorrectors 0;//0;

    pRefCell            0;
    pRefValue           0;
}

relaxationFactors
{
	p		0.3;
	U		0.5;
	k		0.7;
	epsilon	0.7;
/*  equations
    {
        ".*"            1;
    }
    */
}


